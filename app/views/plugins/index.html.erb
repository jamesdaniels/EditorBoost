<h1>All plugins for <%= @editor.name %></h1>

<table>
  <tr>
    <th>Name</th>
  </tr>
  <% @plugins.each do |plugin| %>
    <tr>
      <td><%= link_to plugin.name, plugin_path(plugin, :editor => @editor.short) %></td>
    </tr>
  <% end %>
</table>

<br />

<%= link_to 'New Plugin', new_plugin_path(:editor => @editor.short) %>
